<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <!--
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
            -->
            <PatternLayout pattern="{ &quot;ver&quot;:&quot;0.1&quot;, &quot;message&quot; : &quot;%d{yyyy-MM-dd'T'HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg&quot;}%n"/>
        </Console>
    </Appenders>
    <Loggers>

        <!--
        <Root level="debug">
            <AppenderRef ref="Console"/>
        </Root>
        -->
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>